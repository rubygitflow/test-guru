<%= render 'test_title', test: @test %>

<p>
  <b class="page_caption">Body of question: </b>
  <div class="question_body"><%= @question.body %></div>
</p>

<%= render 'answers/index', answers: @question.answers, question: @question %>

<p>
  <div class="navigation_link"><%= link_to 'Edit', edit_question_path(@question) %></div>
  <div class="navigation_link"><%= link_to 'Go to the test', test_path(@question.test) %></div>
</p>
